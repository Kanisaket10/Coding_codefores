#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin >> t;
	while(t--){
	    int n, k;
	    cin >> n >> k;
	    
	    vector<int> arr(n);
	    for(int i=0; i<n; i++) cin >> arr[i];
	    
	    vector<pair<int, int>> new_arr;
	    for(int i=0; i<n; i++) {
	        new_arr.push_back({arr[i]%k, i});
	        
	        if(new_arr[i].first == 0){
	            new_arr[i].first = k;
	        }
	           
	    }     
	    
	    sort(new_arr.begin(), new_arr.end(), [&](pair<long long, long long> a, pair<long long, long long> b) {
			if (a.first != b.first)
				return a.first > b.first;
			return a.second < b.second;
		});
	    
	    for(int i=0; i<n; i++){
	        cout<< new_arr[i].second+1 << " ";
	    }
	    cout<<endl;
	}

}
